{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "category": "DeveloperTool",
    "copyright": "Copyright Scrab 2023. All rights reserved.",
    "targets": ["deb", "rpm", "nsis", "msi"],
    "linux": {
      "deb": {
        "depends": ["libwebkit2gtk-4.1-0", "libgtk-3-0"]
      },
      "rpm": {
        "depends": ["webkit2gtk4.1", "gtk3"]
      }
    }
  },
  "productName": "SexLab Scene Builder",
  "mainBinaryName": "SexLab Scene Builder",
  "version": "2.0.0",
  "identifier": "com.SexLabSceneBuilder.dev",
  "plugins": {
    "cli": {
      "description": "CLI for SLSB",
      "longDescription": "Can convert and serialize traditional SLAL packs to SLSB.",
      "subcommands": {
        "convert": {
          "args": [
            {
              "name": "in",
              "takesValue": true,
              "short": "i"
            },
            {
              "name": "out",
              "takesValue": true,
              "short": "d"
            }
          ]
        },
        "build": {
          "args": [
            {
              "name": "in",
              "takesValue": true,
              "short": "i"
            },
            {
              "name": "out",
              "takesValue": true,
              "short": "o"
            }
          ]
        }
      }
    }
  },
  "app": {
    "withGlobalTauri": false,
    "security": {
      "csp": null
    }
  }
}